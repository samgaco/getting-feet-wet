<h1>User#new</h1>

<h1> Create a New user </h1>

<%= form_for @user, url: {action: "new"} do  |f| %>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
    <br/>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
    <br/>
    <%= f.submit %><br />

    <%= link_to 'back' ,users_index_path %>
<% end %>